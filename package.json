    "vuex": "^2.1.1"
  },
  "devDependencies": {
    "browserify": "^13.1.1",
    "browserify-hmr": "^0.3.5",
    "cross-env": "^3.1.3",
    "envify": "^4.0.0",
    "http-server": "^0.9.0",
    "node-sass": "^3.13.1",
    "npm-run-all": "^3.1.2",
    "uglify-js": "^2.7.5",
    "vueify": "^9.4.0",
    "watchify": "^3.8.0"
  },
  "browserify": {
    "transform": [
      "vueify"
    ]
  }
}
